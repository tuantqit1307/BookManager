<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="aaa">
        <item name="android:fontFamily">@font/nunitosemibold</item>
        <item name="android:textSize">20sp</item>
    </style>
    <style name="AlertDialog" parent="ThemeOverlay.AppCompat.Dialog.Alert">
        <item name="android:fontFamily">@font/nunitosemibold</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textStyle">bold</item>
       <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>
    <style name="Toolbar" parent="Base.TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:fontFamily">@font/nunitosemibold</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:actionMenuTextColor">@android:color/white</item>
        <item name="android:layout_centerHorizontal">true</item>>
    </style>
    <style name="myCustomMenuTextAppearance" parent="@android:style/TextAppearance.Widget.IconMenu.Item">
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="bbbb">
        <item name="android:fontFamily">@font/nunitosemibold</item>
        <item name="android:textSize">30sp</item>
    </style>
    <style name="FloatingActionButton" parent="Widget.Design.FloatingActionButton">
        <item name="android:background">@drawable/ic_add</item>
    </style>
    <style name="SpinnerItem" parent="TextAppearance.AppCompat.Widget.TextView.SpinnerItem">
        <item name="android:fontFamily">@font/nunitosemibold</item>
        <item name="android:textSize">20sp</item>
    </style>
</resources>
